{
  "manifest_version": 2,
  "name": "query-path",
  "version": "1.0",
  "description": "chrome extension for query path project",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "css": [
        "index.css"
      ]
    }
  ]
}

